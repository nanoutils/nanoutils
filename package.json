{
  "name": "nanoutils",
  "version": "0.2.0",
  "description": "Tiniest JavaScript utils library",
  "main": "cjs/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "repository": "https://github.com/nanoutils/nanoutils",
  "author": "Kelin2025 <kelin2025@yandex.ru>",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "yarn size && yarn lint && git add SIZES.md SIZES_CJS.md"
    }
  },
  "scripts": {
    "build:create-index": "rimraf lib/index.js && node ./node_modules/create-index/dist/bin/create-index.js --ignoreUnsafe lib",
    "build": "yarn build:create-index && yarn build:cjs",
    "build:cjs": "rimraf cjs && babel lib --out-dir cjs --ignore \"lib/**/*.test.js\",\"lib/**/*.performance.js\"",
    "dev": "jest --watchAll --verbose",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:publish": "yarn docs:build && node ./misc/cli/docs-publish.js",
    "lint": "eslint --fix ./lib/*/index.js",
    "method:add": "node ./misc/cli/add-method.js",
    "method:check": "node ./misc/cli/check-methods.js",
    "method:rename": "node ./misc/cli/rename-method.js",
    "prepublish": "yarn build",
    "release": "np",
    "size": "node ./misc/cli/size.js",
    "test": "env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" mocha",
    "time": "node ./misc/cli/time.js"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-function-name": "^7.4.4",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-shorthand-properties": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-typescript": "^7.3.3",
    "@size-limit/preset-small-lib": "^4.5.0",
    "@types/mocha": "^9.1.0",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "chalk": "^2.3.0",
    "create-index": "2.5.0",
    "earljs": "^0.1.12",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "gh-pages": "^1.2.0",
    "ghooks": "^2.0.2",
    "mocha": "^9.1.3",
    "np": "^3.0.4",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "size-limit": "4.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "vuepress": "^0.14.1"
  }
}
