{
  "name": "nanoutils",
  "version": "0.1.2",
  "description": "Tiniest JavaScript utils library",
  "main": "cjs/index.js",
  "module": "lib/index.js",
  "repository": "https://github.com/nanoutils/nanoutils",
  "author": "Kelin2025 <kelin2025@yandex.ru>",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "yarn size && yarn lint && git add SIZES.md"
    }
  },
  "scripts": {
    "build:create-index": "cd ./node_modules/create-index && yarn && yarn build && cd ../../",
    "build": "rimraf lib/index.js && yarn build:create-index && node ./node_modules/create-index/dist/bin/create-index.js --ignoreUnsafe lib && yarn build:cjs && yarn size",
    "build:cjs": "babel lib --out-dir cjs --ignore test.js",
    "dev": "jest --watchAll --verbose",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:publish": "yarn docs:build && node ./misc/cli/docs-publish.js",
    "lint": "eslint --fix ./lib/*/index.js",
    "method:add": "node ./misc/cli/add-method.js",
    "method:check": "node ./misc/cli/check-methods.js",
    "method:rename": "node ./misc/cli/rename-method.js",
    "prepublish": "yarn build",
    "release": "np",
    "size": "node ./misc/cli/size.js",
    "test": "jest",
    "time": "node ./misc/cli/time.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "chalk": "^2.3.0",
    "create-index": "git+https://git@github.com/Beraliv/create-index.git#master",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.62.0",
    "gh-pages": "^1.2.0",
    "ghooks": "^2.0.2",
    "jest": "^22.0.4",
    "np": "^3.0.4",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "size-limit": "^0.13.2",
    "vuepress": "^0.14.1"
  }
}
